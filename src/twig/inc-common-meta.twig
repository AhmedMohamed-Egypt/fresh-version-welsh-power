<meta charset="utf-8">
{% if articles|length %}
    <title>{{articles.meta_title|raw}}</title>
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
    <meta name="theme-color" content="#fff">
    <meta
    name="format-detection" content="telephone=no">
    <!-- appstore links-->
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <link rel="manifest" href="/site.webmanifest">
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5">
    <meta name="msapplication-TileColor" content="#2b5797">
    <meta
    name="theme-color" content="#ffffff">
    <!-- seo meta-->
    <meta name="description" content="{{articles.meta_desc|raw}}">
    <meta name="keywords" content="{{articles.meta_keywords|raw}}">
    <meta name="author" content="Welshpower">
{% endif %}
<meta name="csrf-param" content="_csrf">
<meta
name="csrf-token" content="{{csrf|raw}}">
<!-- og meta-->
{% if articles.og_title !='' %}
    <meta property="og:title" content="{{articles.og_title|raw}}">
{% else %}
    <meta property="og:title" content="Welshpower">
{% endif %}
{% if articles.og_url !='' %}
    <meta property="og:url" content="{{articles.og_url|raw}}">
{% else %}
    <meta property="og:url" content="{{siteurl|raw}}">
{% endif %}
<meta property="og:type" content="website">
{% if articles.og_desc !='' %}
    <meta property="og:description" content="{{articles.og_desc|raw}}">
{% else %}
    <meta property="og:description" content="Welshpower">
{% endif %}
{% if articles.og_image !='' %}
    <meta property="og:image" content="{{articles.og_image|raw}}">
    <meta property="og:image:url" content="{{articles.og_image|raw}}">
    <meta property="og:image:secure_url" content="{{articles.og_image|raw}}">
{% else %}
    <meta property="og:image" content="{{siteurl|raw}}/img/og/1_1080х607.jpg">
    <meta property="og:image:url" content="{{siteurl|raw}}/img/og/1_1080х607.jpg">
    <meta property="og:image:secure_url" content="{{siteurl|raw}}/img/og/1_1080х607.jpg"/>
{% endif %}
{% if articles.og_title !='' %}
    <meta name="twitter:title" content="{{articles.og_title|raw}}">
{% else %}
    <meta name="twitter:title" content="Welshpower">
{% endif %}
{% if articles.og_desc !='' %}
    <meta name="twitter:description" content="{{articles.og_desc|raw}}">
{% else %}
    <meta name="twitter:description" content="Welshpower">
{% endif %}
{% if articles.og_image !='' %}
    <meta name="twitter:image:src" content="{{articles.og_image|raw}}">
{% else %}
    <meta name="twitter:image:src" content="{{siteurl|raw}}/img/og/1_1200х628.jpg">
{% endif %}
{% if articles.og_url !='' %}
    <meta name="twitter:url" content="{{articles.og_url|raw}}">
{% else %}
    <meta name="twitter:url" content="{{siteurl|raw}}">
{% endif %}
{% if siteurl=='http://welshpower.3bitlabs.co.uk' %}
    <!-- remove for production-->
    <meta name="robots" content="noindex">
{% endif %}
<link rel="stylesheet" media="all" href="/site/css/app.css">
<link
rel="stylesheet" media="all" href="/site/css/custom.css">
<!--[if lt IE 9]>
<script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
<![endif]-->
