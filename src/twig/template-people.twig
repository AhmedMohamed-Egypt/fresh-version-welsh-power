            {% include "inc-common-header.twig" %}
            {% include "inc-common-mobilenav.twig" %}
            <!-- what should be changed with every pjax-->
            <div class="page">

                <main
                    class="page__content ">

                    <!-- start hero block-->
                    <div class="hero blank ratio ratio-21x9 page-h-offset">
