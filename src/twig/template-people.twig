                            <img src="https://res.cloudinary.com/welshpower/image/upload/f_auto,q_auto/v1631739579/assets/images/article_image/1631739578_11948_people_hero.jpg">
                            <div class="hero--band">
                                <span>&nbsp;</span>
                            </div>
                        </div>
                    </div>
                    <!-- End hero block-->

                    {{ banners[6]|raw }}
                    {% include "inc-peoplelist.twig" %}
                    {{ banners[7]|raw }}
                </main>
                {% include "inc-common-footer.twig" %}
            </div>
            {% include "inc-common-footerscripts.twig" %}
        </div>
